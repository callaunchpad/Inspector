{
    "manifest_version": 2,
    "name": "Inspector",
    "version": "1.0.0",
    "permissions": ["activeTab", "YOUR_GOOGLE_CLOUD_FUNCTION_URL"],
    "description": "Highlights the most important parts of posts/stories/articles!",
    "icons": {
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}